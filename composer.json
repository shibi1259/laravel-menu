{
  "name": "shibi1259/laravel-menu",
  "description": "Laravel Menu Builder | Drag & Drop | Bootstrap | Laravel 7 | Laravel 8 | Laravel 9 | Laravel 10 | Laravel 11",
  "keywords": [
    "Laravel",
    "Menu",
    "Bootstrap",
    "Builder",
    "Drag",
    "Drop"
  ],
  "authors": [
    {
      "name": "Nguyen Dac Huy",
      "email": "huy.cit@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0"
  },
  "autoload": {
    "psr-4": {
      "NguyenHuy\\Menu\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "NguyenHuy\\Menu\\Providers\\MenuServiceProvider"
      ],
      "aliases": {
        "Menu": "NguyenHuy\\Menu\\Facades\\Menu"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
